{
  "name": "personal-game-website-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "description": "A full-stack personal game website.",
  "scripts": {
    "dev:frontend": "npm run dev -w frontend",
    "dev:backend": "npm run dev -w backend",
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm run build -ws --if-present",
    "start:backend": "npm start --workspace=backend",
    "test": "npm test -ws --if-present",
    "prisma:generate": "npm exec prisma generate --workspace=backend",
    "prisma:migrate": "npm exec prisma migrate dev --workspace=backend",
    "prisma:studio": "npm exec prisma studio --workspace=backend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@esbuild/win32-x64": "^0.25.9",
    "@jridgewell/trace-mapping": "^0.3.31",
    "@prisma/debug": "^6.16.1",
    "@prisma/engines-version": "^6.16.0-7.1c57fdcd7e44b29b9313256c76699e91c3ac3c43",
    "@prisma/fetch-engine": "^6.16.1",
    "npm-run-all": "^4.1.5"
  },
  "optionalDependencies": {
    "@esbuild/win32-x64": "^0.25.9"
  }
}
